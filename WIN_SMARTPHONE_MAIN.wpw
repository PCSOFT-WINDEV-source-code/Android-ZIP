        code : |1-
         
         sElement is string = ATT_FULLPATH
         
         Trace("Selected element: [" + ATT_TYPE + "]; " + sElement)
         
         // If it's a folder
         IF ATT_TYPE = frDirectory THEN
         	
         	// Closes the previous archive if necessary
         	IF ARCHIVE_OPENED THEN
         		zipClose("zipArchive")
         		DeleteAll(ARCHIVE_FILES)
         		ARCHIVE_CURRENT 	= ""
         		ARCHIVE_OPENED 	= False
         	END
         	
         	// Lists the elements found in the selected folder
         	ListElements(sElement)
         	
         	
         	// If it is a ZIP archive
         ELSE IF ATT_TYPE = frFile THEN
         	
         	// Closes the previous archive if necessary
         	IF ARCHIVE_OPENED THEN
         		zipClose("zipArchive")
         		DeleteAll(ARCHIVE_FILES)
         		ARCHIVE_CURRENT 	= ""
         		ARCHIVE_OPENED 	= False
         	END
         	
         	// Load the archive
         	zipOpen("zipArchive", sElement)
         	ARCHIVE_CURRENT = sElement
         	ARCHIVE_OPENED = True
         	
         	// Lists the elements found in the archive
         	ListElements("", "zipArchive")	
         	
         	// If it is a folder in the archive
         ELSE IF ATT_TYPE = frZipDirectory THEN
         	
         	// Lists the elements found in the archive
         	ListElements(sElement, "zipArchive")	
         	
         	// If it is a file in the archive
         ELSE IF ATT_TYPE = frZipFile THEN
         	
         	// No action
         END
