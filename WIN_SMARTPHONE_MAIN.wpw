   -
     name : ListElements
     procedure_id : 1760398453437284896
     type_code : 14
     code : |1-
      // Summary: Lists the folders and the archives found in a folder
      // Syntax:
      //ListElements ([<sRoot> is string [, <sArchive> is string]])
      //
      // Parameters:
      //	sRoot (UNICODE string - default value=""): Root of the disk from which the elements must be listed
      // 	sArchive (UNICODE string - default value=""): Name of the current archive
      // Return value:
      // 	None
      //
      PROCEDURE ListElements(LOCAL sRoot is string = "", LOCAL sArchive is string = "")
      
      sListDirectories	is string
      sListFiles		is string
      sDirectory			is string
      sFile			is string
      sParent				is string
      sZIPInfo			is string
      nNbFiles			is int
      sListElements		is string
      arrElements			is array of strings
      arrFolders			is array of strings
      sElement			is string
      sFolder			is string
      nDepth			is int
      sInfoCompFile	is string
      nElementDepth	is int
      
      // Fills
      Trace("Retrieving the list of files and folders of: " + (["("] + sArchive + [") "]) + (sRoot = "" ? "<Root>" ELSE sRoot))
      
      IF sArchive = "" THEN
      	CURRENT_DIR = sRoot
      	STC_PATH = (sRoot = "" ? "No current folder" ELSE [SEPARATOR] + Replace(Replace(sRoot, ARCHIVES, "Archives"), SysDirStorageCard(), "SDCARD"))
      ELSE
      	STC_PATH = [SEPARATOR] + Replace(Replace(ARCHIVE_CURRENT, ARCHIVES, "Archives"), SysDirStorageCard(), "SDCARD") + [SEPARATOR] + sRoot
      END
      
      // Clears the list
      LooperDeleteAll(LOOP_FILES)
      
      // If no current archive
      IF sArchive = "" THEN
      	
      	// If we are positioned at the root
      	IF sRoot = "" THEN
      		
      		Trace("Enabling the root folders...")
      		
      		// Folder of the application
      		LooperAddLine(LOOP_FILES, IMG_EXAMPLE, "Archives", ARCHIVES, frDirectory, "")
      		
      		IF NOT InSimulatorMode THEN
      			// If a SDCARD is enabled
      			IF SysStatusStorageCard() <> sysCardUnavailable THEN
      				// Add the root of the system
      				LooperAddLine(LOOP_FILES, IMG_SDCARD, "SDCARD", SysDirStorageCard(), frDirectory, "")
      			ELSE
      				// Add the root of the system
      				LooperAddLine(LOOP_FILES, IMG_SYSTEM, "System", "/", frDirectory, "")
      			END			
      		END
      	ELSE
      		
      		// Parent path
      		IF sRoot = SysDirStorageCard() _OR_ sRoot = "/" _OR_ sRoot = ARCHIVES THEN
      			sParent = ""
      		ELSE
      			sParent = sRoot[[TO PositionOccurrence(sRoot, SEPARATOR, lastRank) - 1]]
      		END
      		Trace("Parent folder: " + (sParent = "" ? "<Root>" ELSE sParent))
      		
      		// Lists the directories
      		Trace("Finding folders from: " + sRoot)
      		sListDirectories = fListDirectory(sRoot, frNotRecursive)
      		Trace(StringCount(sListDirectories, CR) + " listed folder(s)")
      		
      		// Lists the files
      		Trace("Finding ZIP archives from: " + sRoot)
      		sListFiles	  = fListFile(sRoot + [SEPARATOR] + "*.zip", frNotRecursive)
      		Trace(StringCount(sListFiles, CR) + " listed ZIP archive(s)")
      		
      		// If the folder is empty
      		IF sListFiles = "" _AND_ sListDirectories = "" THEN
      			
      			// Displays a toast
      			ToastDisplay("The folder contains no archive", toastShort, vaMiddle, haCenter)			
      			
      			// Go back to the parent
      			ListElements(sParent)
      		ELSE
      			
      			// For each directory
      			FOR EACH string sDirectory of sListDirectories SEPARATED BY CR		
      				LooperAddLine(LOOP_FILES, IMG_FOLDER, ExtractString(sDirectory, lastRank, SEPARATOR), sDirectory, frDirectory, "")
      			END
      			
      			// For each file
      			FOR EACH string sFile of sListFiles SEPARATED BY CR
      				
      				// Opens the archive
      				IF zipOpen("zipTempArchive", sFile, zipRead) = 0 THEN
      					
      					// Retrieves the number of the files found in the archive
      					nNbFiles = zipNbFile("zipTempArchive")
      					
      					// Displays the size and the number of the files found in the archive
      					sZIPInfo = LengthToString(zipSize("zipTempArchive", True)) + CR + 
      					NumToString(nNbFiles, "dS") + " file" + (nNbFiles > 1 ? "s" ELSE "")
      					
      					// Closes the archive
      					zipClose("zipTempArchive")
      				ELSE
      					sZIPInfo = "Invalid archive"
      				END
      				
      				LooperAddLine(LOOP_FILES, IMG_ARCHIVE, fExtractPath(sFile, fFileName + fExtension), sFile, frFile, sZIPInfo)
      			END
      			
      			// Adds an element to move up in the tree structure
      			LooperInsertLine(LOOP_FILES, 1, IMG_PARENT_FOLDER, "", sParent, frDirectory, "")
      		END
      	END
      ELSE
      	
      	IF sRoot = "" THEN
      		
      		// Adds an element to move up in the tree structure
      		LooperAddLine(LOOP_FILES, IMG_PARENT_FOLDER, "", UncompleteDir(fExtractPath(ARCHIVE_CURRENT, fDrive + fDirectory)), frDirectory, "")				
      		
      		// Stores the archive files
      		IF ARCHIVE_FILES..Occurrence = 0 THEN
      			
      			// Lists the files found in the archive
      			sListElements = zipListFile("zipArchive")
      			// Transforms the list into table
      			StringToArray(sListElements, arrElements, CR)
      			// Sorts the array
      			Sort(arrElements, asAscending)
      			
      			FOR EACH sElement of arrElements
      				
      				// Extracts the element folder
      				sFolder = UncompleteDir(fExtractPath(sElement, fDrive + fDirectory))
      				
      				// If the folder is empty, we are positioned at the root of the archive, therefore we ignore this folder
      				IF sFolder <> "" THEN					
      					// If the folder was not stored yet
      					IF Seek(arrFolders, asLinear, sFolder) < 1 THEN
      						Add(arrFolders, StringCount(sFolder, SEPARATOR) + ";" + frZipDirectory + ";" + sFolder)
      					END										
      					
      					// Adds the file from the folder
      					Add(ARCHIVE_FILES, StringCount(sFolder + [SEPARATOR], SEPARATOR) + ";" + frZipFile + ";" + sElement)
      				ELSE
      					// Adds the file of the root
      					Add(ARCHIVE_FILES, "0;" + frZipFile + ";" + sElement)
      				END
      				
      			END
      			
      			// Inserts the folders at the beginning of the list
      			Insert(ARCHIVE_FILES, 1, arrFolders)
      			Sort(ARCHIVE_FILES, asAscending)
      		END
      		
      		// No depth
      		nDepth = 0
      	ELSE
      		// Requested level of depth
      		nDepth	= StringCount(sRoot, SEPARATOR) + 1
      		
      		// Adds an element to move up in the tree structure
      		sParent = sRoot[[TO PositionOccurrence(sRoot, SEPARATOR, lastRank) - 1]]
      		LooperAddLine(LOOP_FILES, IMG_PARENT_FOLDER, "", sParent, frZipDirectory, "")				
      	END
      	
      	// Lists the folders at the root
      	FOR EACH sElement of ARCHIVE_FILES
      		
      		nElementDepth = Val(ExtractString(sElement, 1, ";"))
      		
      		// If the listed element does not belong to the requested level of depth
      		IF nElementDepth > nDepth THEN
      			BREAK
      		ELSE IF nElementDepth < nDepth THEN
      			CONTINUE
      		END
      		
      		// According to the type of the element
      		SWITCH Val(ExtractString(sElement, 2, ";"))
      		
      			// It's a directory
      			CASE frZipDirectory
      				
      				sDirectory = ExtractString(sElement, lastRank, ";")
      				
      				// Checks whether the directory is found in the current root
      				IF NOT sDirectory[[TO PositionOccurrence(sDirectory, SEPARATOR, lastRank) - 1]] = sRoot THEN
      					CONTINUE
      				END
      				
      				LooperAddLine(LOOP_FILES, IMG_FOLDER, ExtractString(sDirectory, lastRank, SEPARATOR), sDirectory, frZipDirectory, "")
      				
      			// It's a file
      			CASE frZipFile
      				
      				sFile  = ExtractString(sElement, lastRank, ";")
      				
      				// Checks whether the file is found in the current root
      				IF NOT UncompleteDir(fExtractPath(sFile, fDrive + fDirectory)) = sRoot THEN
      					CONTINUE
      				END
      				
      				sInfoCompFile = zipInfoFile(sArchive, sFile)					
      				sZIPInfo = LengthToString(Val(ExtractString(sInfoCompFile, 2))) + CR +
      				"Ratio: " + Round((Val(ExtractString(sInfoCompFile, 3)) * 100 / max(1, Val(ExtractString(sInfoCompFile, 2)))), 1) + "%"
      				
      				LooperAddLine(LOOP_FILES, IMG_FILE, fExtractPath(sFile, fFileName + fExtension), sFile, frZipFile, sZIPInfo)
      		END
      	END			
      	
      	
      END
     type : 458752
   -
     name : ExtractInto
     procedure_id : 1760398453437350432
     type_code : 14
     code : |1-
      // Summary: Extracts the archive that is currently opened into the specified directory
      // Syntax:
      //ExtractInto (<sExtractionDir> is string)
      //
      // Parameters:
      //	sExtractionDir (UNICODE string): Directory into which the archive will be extracted
      // Return value:
      // 	None
      //
      PROCEDURE ExtractInto(LOCAL sExtractionDir is string)
      
      nSubscript 			is int
      nErr				is int
      sExtractionDirBkp	is string = sExtractionDir
      
      EXTRACTIONDIR = ""
      OpenMobileWindow(WIN_PROGBAR_EXTRACTION)
      
      // Full extraction of the archive
      IF ATT_TYPE = frFile THEN
      	
      	Trace("Extraction of " + ATT_FULLPATH + " in: " + sExtractionDirBkp)
      	nErr = zipOpen("zipTempArchive", ATT_FULLPATH, zipRead)
      	IF nErr = 0 THEN
      		
      		HourGlass(True)
      		
      		WIN_PROGBAR_EXTRACTION.PROGBAR_EXTRACTION = 0
      		WIN_PROGBAR_EXTRACTION.PROGBAR_EXTRACTION..MaxValue = zipNbFile("zipTempArchive")
      		WIN_PROGBAR_EXTRACTION.PROGBAR_EXTRACTION..Visible = True
      		
      		FOR i = 1 _TO_ WIN_PROGBAR_EXTRACTION.PROGBAR_EXTRACTION..MaxValue
      			
      			// Informs about the file currently extracted
      			WIN_PROGBAR_EXTRACTION.STC_INFO_EXTRACTION = "Extract the file" + CR + ExtractString(zipInfoFile("zipTempArchive", i), 1) + "..."
      			
      			// Checks out the file
      			Trace("Extraction of " + ExtractString(zipInfoFile("zipTempArchive", i), 1) + " in: " + sExtractionDirBkp)
      			nErr = zipExtractFile("zipTempArchive", i, sExtractionDirBkp)
      			IF nErr <> 0 THEN
      				Error("Unable to check out the file", zipMsgError(nErr))
      				BREAK
      			END
      			WIN_PROGBAR_EXTRACTION.PROGBAR_EXTRACTION ++
      		END
      		
      		WIN_PROGBAR_EXTRACTION.STC_INFO_EXTRACTION = ""
      		zipClose("zipTempArchive")
      		
      		HourGlass(False)
      	ELSE
      		Error("Unable to extract the archive", zipMsgError(nErr))
      	END		
      	
      	// Extract a file from the archive
      ELSE IF ATT_TYPE = frZipFile THEN
      	
      	HourGlass(True)
      	
      	// Informs about the file currently extracted
      	Trace("Extraction of " + ATT_FULLPATH + " in: " + sExtractionDirBkp)
      	WIN_PROGBAR_EXTRACTION.STC_INFO_EXTRACTION = "Extract the file" + CR + ATT_FULLPATH + "..."
      	
      	nErr = zipExtractFile(ARCHIVE_CURRENT, ATT_FULLPATH, sExtractionDirBkp)
      	IF nErr <> 0 THEN Error("Unable to check out the file", zipMsgError(nErr))
      	
      	WIN_PROGBAR_EXTRACTION.STC_INFO_EXTRACTION = ""
      	HourGlass(False)
      	
      	// Extract a full directory from the archive
      ELSE IF ATT_TYPE = frZipDirectory THEN
      	
      	HourGlass(True)
      	
      	Trace("Extraction of " + ATT_FULLPATH + " in: " + sExtractionDirBkp)
      	
      	// First file of the directory
      	nSubscript = zipFindFile(ARCHIVE_CURRENT, ATT_FULLPATH, False, 1)
      	WHILE nSubscript > 0
      		
      		// Informs about the file currently extracted
      		WIN_PROGBAR_EXTRACTION.STC_INFO_EXTRACTION = "Extract the file" + CR + ExtractString(zipInfoFile(ARCHIVE_CURRENT, nSubscript), 1) + "..."
      		
      		// Checks out the file
      		Trace("Extraction of " + ExtractString(zipInfoFile(ARCHIVE_CURRENT, nSubscript), 1) + " in: " + sExtractionDirBkp + ATT_CAPTION)
      		nErr = zipExtractFile(ARCHIVE_CURRENT, nSubscript, sExtractionDirBkp + [SEPARATOR] + ATT_CAPTION)
      		IF nErr <> 0 THEN
      			Error("Unable to check out the file " + ExtractString(zipInfoFile(ARCHIVE_CURRENT, nSubscript), 1), zipMsgError(nErr))
      			BREAK
      		END
      		
      		// Next file
      		nSubscript = zipFindFile(ARCHIVE_CURRENT, ATT_FULLPATH, False, nSubscript + 1)
      	END
      	
      	WIN_PROGBAR_EXTRACTION.STC_INFO_EXTRACTION = ""
      	HourGlass(False)
      	
      END
      
      Trace("End of extraction")
      Close(WIN_PROGBAR_EXTRACTION)
